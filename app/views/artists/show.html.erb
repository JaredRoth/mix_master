<h1><%= @artist.name %></h1>
<p><%= image_tag @artist.image_path %></p>
<ul id="songs">
  <% @artist.songs.each do |song| %>
    <li>
      <%= link_to song.title, song_path(song), class: 'song_title' %>
    </li>
  <% end %>
</ul>
<p><%= link_to "Edit", edit_artist_path %></p>
<p><%= link_to "Delete", artist_path(@artist), method: :delete %></p>
<p><%= link_to "New Song", new_artist_song_path(@artist) %></p>
